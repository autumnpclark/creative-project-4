se = await axios.get(\"/api/posts\");\r\n                this.posts = this.response.data;\r\n            } catch (error) {\r\n                this.error = error.response.data.message;\r\n            }\r\n        },\r\n        close() {\r\n            this.show = false;\r\n        },\r\n        toggleUpload() {\r\n            this.show = true;\r\n        },\r\n        async uploadFinished() {\r\n            this.show = false;\r\n            this.getPosts();\r\n        },\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\nbutton {\r\n    height: 80px;\r\n    width: 80px;\r\n    font-size: 3.2em;\r\n    background-color:#2B425C;\r\n    margin: 20px;\r\n}\r\n\r\n.posts {\r\n    display:flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n}\r\n\r\n.one-post {\r\n    height: 300px;\r\n    width: 80%;\r\n    background-color: white;\r\n    display: flex;\r\n    flex-direction: row;\r\n    padding: 20px;\r\n    margin:15px;\r\n}\r\n\r\n.one-post img {\r\n    max-height: 250px;\r\n    width: auto;\r\n}\r\n\r\n.one-post a {\r\n    display: flex;\r\n    flex-direction: row;\r\n    text-decoration: none;\r\n}\r\n\r\n.post-info {\r\n    padding: 15px;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}