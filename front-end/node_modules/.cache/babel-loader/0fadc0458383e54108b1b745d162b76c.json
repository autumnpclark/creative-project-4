AA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,yBAAA,CAAA,CAAA,IAAA,IAAA,KAAA,CAAA,IAAA;AAAA,iBAAA,CAAA;AAJA;AAAA;;AAAA;AAAA;AAAA;AAMA,gBAAA,KAAA,CAAA,KAAA,GAAA,YAAA,QAAA,CAAA,IAAA,CAAA,OAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KATA;AAUA,IAAA,KAVA,mBAUA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,KAZA;AAaA,IAAA,YAbA,0BAaA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,KAfA;AAgBA,IAAA,UAhBA,sBAgBA,IAhBA,EAgBA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,CAAA,MAAA,CAAA,cAAA,CAAA;AACA,KAlBA;AAmBA,IAAA,cAnBA,4BAmBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,IAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA;AAtBA;AApBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"food\">\r\n        <div class=\"addPost\">\r\n            <button v-if=\"user\" class=\"w3-button w3-black\" @click=\"toggleUpload\">+</button>\r\n            <upload v-bind:type=\"type\" :show=\"show\" @close=\"close\" @uploadFinished=\"uploadFinished\" />\r\n            <p>If you can't eat well, eat often</p>\r\n        </div>\r\n        <div class=\"posts\">\r\n            <div class=\"one-post\"  v-for=\"post in posts\" v-bind:key=\"post._id\">\r\n                <router-link :to=\" '/post/' + post._id\">\r\n                <div class=\"post-photo\">\r\n                    <img :src=\"post.path\" />\r\n                </div>\r\n                <div class=\"post-info\">\r\n                    <div class=\"post-header\">\r\n                        <h2>{{post.title}}</h2>\r\n                        <p>{{post.location}}</p>\r\n                    </div>\r\n                    <p id=\"blurb\">{{post.blurb.substring(0,240) + '...'}}</p>\r\n                    <p>Posted on {{formatDate(post.created)}} by {{post.user.firstName}}</p>\r\n                </div>\r\n                </router-link>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport moment from 'moment';\r\nimport axios from 'axios';\r\nimport Upload from '@/components/Upload.vue';\r\nexport default {\r\n    name: 'Food',\r\n    components: {\r\n        Upload\r\n    },\r\n    data() {\r\n        return {\r\n            show: false,\r\n            type: \"Food\",\r\n            posts: []\r\n        }\r\n    },\r\n    created() {\r\n        this.getPosts();\r\n    },\r\n    computed: {\r\n      user() {\r\n        return this.$root.$data.user;\r\n      },\r\n    },\r\n    methods: {\r\n        async getPosts() {\r\n            try {\r\n                this.response = await axios.get(\"/api/posts\");\r\n                this.posts = this.response.data;\r\n                this.posts = this.posts.filter(x => x.type == this.type);\r\n            } catch (error) {\r\n                this.error = error.response.data.message;\r\n            }\r\n        },\r\n        close() {\r\n            this.show = false;\r\n        },\r\n        toggleUpload() {\r\n            this.show = true;\r\n        },\r\n        formatDate(date) {\r\n            return moment(date).format('DD MMMM YYYY');\r\n        },\r\n        async uploadFinished() {\r\n            this.show = false;\r\n            this.getPosts();\r\n        },\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n\r\n\r\nbutton {\r\n    height: 80px;\r\n    width: 80px;\r\n    font-size: 3.2em;\r\n    background-color: #2B425C;\r\n    margin: 20px;\r\n    padding: 0;\r\n    padding-bottom: 6px;\r\n}\r\n\r\n.posts {\r\n    display:flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n}\r\n\r\n.one-post {\r\n    height: max-content;\r\n    width: 80%;\r\n    background-color: white;\r\n    border-radius: 5px;\r\n    display: flex;\r\n    flex-direction: row;\r\n    padding: 20px;\r\n    margin:15px;\r\n}\r\n\r\n.post-photo {\r\n    display:flex;\r\n    align-self: center;\r\n    justify-content: center;\r\n    margin: 0px 20px;\r\n}\r\n\r\n.one-post img {\r\n    max-height: 250px;\r\n    width: auto;\r\n    max-width: 500px;\r\n    border-radius: 5px;\r\n}\r\n\r\n.one-post a {\r\n    display: flex;\r\n    flex-direction: row;\r\n    text-decoration: none;\r\n}\r\n\r\n.post-info {\r\n    padding: 15px;\r\n}\r\n\r\n.addPost {\r\n    display: flex;\r\n    flex-direction: row;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n\r\n#blurb {\r\n    font-size: 1.3em;\r\n}\r\n\r\n.addPost p {\r\n    display: flex;\r\n    width:fit-content;\r\n    font-size: 3em;\r\n}\r\n\r\n@media screen and (max-width: 800px) {\r\n    .addPost {\r\n        flex-direction: column;\r\n    }\r\n\r\n    .one-post a{\r\n        flex-direction: column;\r\n    }\r\n}\r\n\r\n</style>"],"sourceRoot":"src/views"}]}